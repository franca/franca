    <eStructuralFeatures xsi:type="ecore:EReference" name="methods" upperBound="-1"
        eType="#//FMethod" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="broadcasts" upperBound="-1"
