  <eClassifiers xsi:type="ecore:EClass" name="FArrayType" eSuperTypes="#//FType">
    <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
      <details key="documentation" value="This class represents an array type in Franca.&#xA;The array type is used for defining an explicitly named type&#xA;which represents an array of a given element type."/>
